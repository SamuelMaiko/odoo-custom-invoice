[options]
; Admin password for Odoo (replace with your own) 
admin_passwd = your_admin_password_here

; Database connection settings
db_host = localhost
db_port = 5432
db_user = your_db_user_here
db_password = your_db_password_here
db_name = your_db_name_here

; Addons paths (comma-separated)
addons_path = /path/to/odoo/addons, /path/to/odoo/odoo/addons, /path/to/custom/addons

; HTTP server port
http_port = 8001
